<Application
    x:Class="QuickPad.UI.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:QuickPad.UI"
    xmlns:mvvm="using:QuickPad.MVVM">

    <Application.Resources>
        <ResourceDictionary >
            <ResourceDictionary.MergedDictionaries >
                <ResourceDictionary Source="/ResourceDictionaries/RichEditBox.xaml"/>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls"/>
            </ResourceDictionary.MergedDictionaries>
            <mvvm:QuickPadCommands x:Key="QuickPadCommands"/>
            <mvvm:SettingsViewModel x:Key="SettingsViewModel" />
        </ResourceDictionary>
        
    </Application.Resources>
    
</Application>
